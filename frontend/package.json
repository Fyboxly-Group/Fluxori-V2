{
  "name": "fluxori-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:chakra": "eslint . --ext .ts,.tsx --rule 'chakra-ui-v3/imports: error' --rule 'chakra-ui-v3/props: error' --rule 'chakra-ui-v3/types: error'",
    "typecheck": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "format": "prettier --write .",
    "validate": "npm run lint && npm run typecheck",
    "build:production": "NODE_ENV=production npm run validate && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install",
    "gen:chakra-types": "node scripts/generate-chakra-types.js",
    "fix:ts": "node scripts/fix-frontend-ts-errors.js",
    "dev:nocheck": "NEXT_SKIP_TYPECHECKING=1 next dev",
    "build:nocheck": "NEXT_SKIP_TYPECHECKING=1 next build",
    "dev:safe": "NEXT_SKIP_TYPECHECKING=1 NEXT_SKIP_ESLINT=1 next dev",
    "build:safe": "NEXT_SKIP_TYPECHECKING=1 NEXT_SKIP_ESLINT=1 next build",
    "build:dev": "NODE_ENV=development NEXT_PUBLIC_SKIP_BUILD_CHECKS=1 npm run build:safe",
    "check-chakra-v3": "npm run typecheck && echo 'All TypeScript checks pass with Chakra UI V3!'"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.14.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@tanstack/react-query": "^5.70.0",
    "chart.js": "^4.4.8",
    "framer-motion": "^12.6.2",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.486.0",
    "next": "^15.2.4",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.55.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.17.28",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.5",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.2.4",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "source-map-loader": "^5.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.8.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "tsc-files --noEmit"
    ]
  },
  "alias": {
    "@chakra-ui/react/form-control": "./src/utils/fallbacks/form-control.js"
  }
}
