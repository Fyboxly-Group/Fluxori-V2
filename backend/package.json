{
  "name": "fluxori-v2-backend",
  "version": "1.0.0",
  "description": "Backend API for Fluxori-V2",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "typecheck:src": "tsc --noEmit --skipLibCheck --excludeFiles \"src/**/*.test.ts\" --excludeFiles \"src/tests/**/*.ts\"",
    "typecheck:core": "tsc --noEmit --project tsconfig.core.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "jest --testMatch='**/integration/**/*.test.ts'",
    "test:ci": "jest --ci --coverage --json --outputFile=coverage/jest-results.json && node scripts/test-summary.js",
    "test:summary": "node scripts/test-summary.js",
    "clean": "rimraf dist"
  },
  "keywords": [
    "api",
    "backend",
    "express",
    "typescript",
    "fluxori"
  ],
  "author": "Fluxori Team",
  "license": "UNLICENSED",
  "dependencies": {
    "@google-cloud/aiplatform": "^4.1.0",
    "@google-cloud/storage": "^7.15.2",
    "@types/axios": "^0.9.36",
    "@types/multer": "^1.4.12",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^13.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.0",
    "multer": "^1.4.5-lts.2",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^20.10.5",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
